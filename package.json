{
  "name": "source-beb",
  "version": "1.0.2",
  "description": "Create a Back-End Project with Node.js, Express, and MongoDB in one command.",
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "backend",
    "backend-builder",
    "project-generator",
    "cli",
    "generator"
  ],
  "main": "./dist/index.js",
  "type": "commonjs",
  "bin": {
    "beb": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "xcopy /E /I /Y src\\templates dist\\templates",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.10.9",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^14.14.6",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.1.2"
  }
}
